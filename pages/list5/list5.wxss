/* pages/list5/list5.wxss */
.page {
  padding: 0 5px 0 5px;
  display: grid;
  padding-bottom: 20px;
}
.ziti{
  width: 100%;
  height: 110rpx;
  background-color: #fdfdfb;
  text-align: center; 
}
.ziti>text{
  font-size: 50rpx;
  font-weight: bold;
  color: #e6b820;
  line-height: 110rpx;
  font-style: unset;
}

.title{
  display: flex;
  height: 35px;
  align-items: center;
  padding-left: 5px;
}
.titleTxt {
  font-size: 1.3em;
  margin-left: 8px;
}
.txt {
  font-size: 18px;
  margin: 5px;
  text-indent: 2em;
}
.subTitle {
  font-weight: 550;
  /* font-family: monospace; */
  font-size: 1.2em;
}
.quare{
  width: 40rpx;
  height: 40rpx;
  background-color: #EEAD0E;
}
.img {
  /* width: 35vh; */
  height: 50vh;
  border-radius: 2ch;
}
.imgContainer {
  width: 100%;
  display: flex;
  justify-content: center;
  margin: 10px 0 10px 0;
}